<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="mainAnchorPane" maxHeight="1080"
	maxWidth="1920" minHeight="400" minWidth="600"
	prefHeight="1080.0" prefWidth="1920.0"
	xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="scenes_controller.GameOverController">
	<children>
		<HBox prefHeight="100.0" prefWidth="200.0"
			spacing="200.0" AnchorPane.bottomAnchor="490.0"
			AnchorPane.leftAnchor="495.0" AnchorPane.rightAnchor="495.0"
			AnchorPane.topAnchor="500.0">
			<children>
				<Button fx:id="menuButton" onAction="#switchToMenuScene" mnemonicParsing="false"
					prefHeight="90.0" prefWidth="150.0" text="MENU" />
				<Button fx:id="restartButton" onAction="#restartLevel" mnemonicParsing="false"
					prefHeight="90.0" prefWidth="150.0" text="RESTART" />
			</children>
		</HBox>
	</children>
</AnchorPane>


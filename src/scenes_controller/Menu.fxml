<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.control.Button?>


<AnchorPane maxHeight="1080"
	maxWidth="1920" minHeight="400" minWidth="600" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/21"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="scenes_controller.MenuController">
	<children>
		<FlowPane alignment="CENTER" columnHalignment="CENTER"
			layoutX="200.0" layoutY="51.2" orientation="VERTICAL"
			prefHeight="226.0" prefWidth="300.0" vgap="40.0"
			AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="100.0"
			AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="60.0">
			<children>
				<Button fx:id="playingButton" mnemonicParsing="false"
					onAction="#switchScene" text="PLAY" />
				<Button  fx:id="editingButton" mnemonicParsing="false"
					onAction="#switchScene" text="EDIT" />
				<Button  fx:id="settingsButton" mnemonicParsing="false"
					onAction="#switchScene" text="SETTINGS" />
				<Button  fx:id="quitButton" mnemonicParsing="false"
					onAction="#closeApplication" text="QUIT" />
			</children>
		</FlowPane>
	</children>
</AnchorPane>

